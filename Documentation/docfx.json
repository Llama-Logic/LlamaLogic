{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "LlamaLogic.Packages/*.csproj"
          ]
        }
      ],
      "dest": "packages"
    },
    {
      "src": [
        {
          "src": "../",
          "files": [
            "LlamaLogic.ValveDataFormat/*.csproj"
          ]
        }
      ],
      "dest": "valve-data-format"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "logo.svg",
          "images/**"
        ]
      }
    ],
    "output": "../docs",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "Llama Logic",
      "_appTitle": "Llama Logic",
      "_enableSearch": true,
      "pdf": true
    }
  }
}